  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      "entity",
      ".contract.ts",
      ".constants.ts",
      ".decorator.ts"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
