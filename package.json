  "scripts": {
    "prebuild": "rimraf dist",
    "build:tabt-rest": "nest build tabt-rest",
    "build:app-notifications": "nest build app-notifications",
    "build:data-aftt-importer": "nest build data-aftt-importer",
    "ci": "node ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:tabt-rest": "node dist/tabt-rest/main",
    "start:dev:data-aftt-importer": "node ./node_modules/@nestjs/cli/bin/nest.js start data-aftt-importer --watch",
    "start:dev:tabt-rest": "nest start tabt-rest --watch",
    "start:dev:app-notifications": "nest start app-notifications --watch",
    "start:debug:tabt-rest": "nest start tabt-rest --debug --watch",
    "start:prod:tabt-rest": "node dist/apps/tabt-rest/main",
    "start:prod:data-aftt-importer": "node dist/apps/data-aftt-importer/main",
    "start:prod:app-notifications": "node dist/apps/app-notifications/main",
    "lint": "eslint \"{apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm apps/tabt-rest/test -- --config test-e2e/jest-e2e.json",
    "release": "release-it",
    "start:azure": "npm run build && func host start",
    "prisma:deploy": "prisma migrate deploy",
    "start:migrate:prod": "npm run prisma:deploy && npm run start:prod",
    "tabt-generate": "openapi-generator-cli generate -i http://localhost:3050/doc-json --skip-validate-spec --generator-name typescript-nestjs -o apps/app-notifications/src/common/tabt-client",
    "prisma:generate": "prisma generate"
  },
